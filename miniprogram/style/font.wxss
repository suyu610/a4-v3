@font-face {
  font-family: "Poppins-title";
  src: url("https://cdns.qdu.life/a4/font/Poppins-title.ttf");
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "Poppins-italic";
  src: url("https://cdns.qdu.life/a4/font/Poppins-italic.ttf");
  font-weight: 300;
  font-style: italic;
  font-display: swap;
}

@font-face {
  font-family: "Poppins-normal";
  src: url("https://cdns.qdu.life/a4/font/Poppins-normal.ttf");
  font-weight: 300;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "Inter";
  src: url("https://cdns.qdu.life/a4/font/Inter.ttf");
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "SF-Pro-Display-BoldItalic";
  src: url("https://cdns.qdu.life/a4/font/SF-Pro-Display-BoldItalic.otf");
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: "share-title-pangmen";
  src: url("data:application/x-font-ttf;charset=utf-8;base64,")
    format("truetype"); /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
  font-weight: 900;
  font-style: italic;
  font-display: swap;
}

@font-face {
  font-family: "SF Pro Display Num";
  src: url(data:application/x-font-ttf;charset=utf-8;base64,)
      format("truetype"),
    /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
      url("SFProDisplay-HeavyItalic.svg#SFProDisplay-HeavyItalic") format("svg"); /* iOS 4.1- */
  font-style: normal;
  font-weight: normal;
}

